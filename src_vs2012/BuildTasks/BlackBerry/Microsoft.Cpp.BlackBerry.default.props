<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <Platform>BlackBerry</Platform>
    <PlatformToolset Condition="'$(PlatformToolset)' == ''">qcc</PlatformToolset>
    <DefaultPlatformToolset Condition="'$(DefaultPlatformToolset)' == ''">qcc</DefaultPlatformToolset>
    <ConfigurationAppType Condition="'$(ConfigurationAppType)' == ''">Regular</ConfigurationAppType>
    <MakefileTargetName Condition="'$(MakefileTargetName)' == ''">all</MakefileTargetName>

    <!-- Short name for platform toolset (appended to project name in Solution Explorer) -->
    <_PlatformToolsetShortNameFor_qcc Condition="'$(_PlatformToolsetShortNameFor_qcc)' == ''">BlackBerry</_PlatformToolsetShortNameFor_qcc>
    <!-- Friendly name for platform toolset (used in project property page) -->
    <_PlatformToolsetFriendlyNameFor_qcc Condition="'$(_PlatformToolsetFriendlyNameFor_qcc)' == ''">BlackBerry QNX Compiler (qcc)</_PlatformToolsetFriendlyNameFor_qcc>
  </PropertyGroup>
</Project>
